{
    "buildCommand": "cd dashboard/frontend && npm install && npm run build",
    "outputDirectory": "dashboard/frontend/.next",
    "installCommand": "cd dashboard/frontend && npm install",
    "framework": "nextjs",
    "functions": {
        "api/**/*.py": {
            "runtime": "@vercel/python@4.5.0",
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.py"
        }
    ]
}